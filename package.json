{
  "name": "node-testcafe",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "babel-watch src/app.js",
    "babel-node": "babel-node --",
    "dist": "./node_modules/.bin/babel src -d dist --presets=babili",
    "dev": "nodemon --exec babel-node src/app.js",
    "test": "testcafe chrome src/tests/index.js --app 'yarn dev' -s screenshots",
    "build": "babel src -d src/dist --copy-files --ignore src/dist"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-es2015-destructuring": "^6.23.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-watch": "2.0.7",
    "body-parser": "^1.18.2",
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "cors": "^2.8.5",
    "eslint": "^4.18.1",
    "express": "^4.16.2",
    "express-handlebars": "^3.0.0",
    "hbs": "^4.0.1",
    "volleyball": "^1.4.1"
  },
  "devDependencies": {
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.9.0",
    "nodemon": "^1.17.1",
    "testcafe": "^1.1.3",
    "testcafe-browser-provider-nightmare": "0.0.4"
  },
  "repository": "https://github.com/seunzone/node-testcafe.git",
  "author": "seunzone <darealseun@gmail.com>",
  "license": "MIT"
}
